{
  "name": "StarWarsSandbox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install:dependencies": "yarn && cd client && yarn",
    "test:eslint": "eslint . --fix",
    "test:flow": "cd client && yarn run test:flow",
    "test:server": "tap test/*.test.js test/*/*.test.js test/*/*/*.test.js",
    "test:client": "cd client && yarn run test",
    "dev:server": "cross-env NODE_ENV=development nodemon ./server.js",
    "dev:client": "cd client && yarn start",
    "build:client": "cd client && yarn && yarn run build",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others-on-fail \"yarn dev:server\" \"yarn dev:client\"",
    "start": "cross-env NODE_ENV=production CI=true node ./server.js"
  },
  "dependencies": {
    "apollo-server": "^2.0.0",
    "apollo-server-express": "^2.0.0",
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-cluster": "^0.0.5",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.1.1",
    "mongoose": "^5.2.7",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-airbnb-flow": "^1.0.2",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "7.9.1",
    "flow-bin": "^0.76.0",
    "flow-remove-types": "^1.2.3",
    "nodemon": "^1.18.3",
    "standard": "^11.0.1",
    "tap": "^12.0.0"
  }
}
